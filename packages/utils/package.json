{
  "name": "@okiss/utils",
  "version": "0.0.5",
  "scripts": {
    "build": "cross-env pkg=utils vite build --config ../../build/lib.config.ts",
    "build:report": "cross-env pkg=utils EXPORT=true vite build --config ../../build/lib.config.ts",
    "dev": "cross-env pkg=utils vite --config ../../build/dev.config.ts",
    "release": "pnpm build && cross-env pkg=utils node ../../build/release.js"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "dayjs": "^1.11.3",
    "element-plus": "^2.2.5",
    "handlebars": "^4.7.7",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.19",
    "@types/rollup-plugin-visualizer": "^4.2.1",
    "@typescript-eslint/parser": "^5.12.1",
    "@vitejs/plugin-vue": "^2.2.2",
    "@vitejs/plugin-vue-jsx": "^1.3.7",
    "cross-env": "^7.0.3",
    "eslint": "^8.9.0",
    "eslint-plugin-vue": "^8.4.1",
    "rollup-plugin-visualizer": "^5.5.4",
    "ts-node": "^10.8.1",
    "typescript": "^4.5.5",
    "unplugin-auto-import": "^0.6.0",
    "vite": "^2.8.4",
    "vite-plugin-dts": "^0.9.9"
  },
  "files": [
    "dist/*"
  ],
  "module": "./src/index.ts",
  "style": "./dist/style.css",
  "types": "./dist/types/index.d.ts",
  "exports": {
    "./style.css": "./dist/style.css",
    ".": {
      "import": "./src/index.ts"
    }
  }
}