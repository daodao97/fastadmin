{
  "name": "@okiss/vbtf",
  "version": "0.0.47",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "cross-env pkg=vbtf vite --config ../../build/dev.config.ts",
    "build": "cross-env pkg=vbtf vite build --config ../../build/lib.config.ts",
    "release": "pnpm build && cross-env pkg=vbtf node ../../build/release.js"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.10",
    "@okiss/utils": "workspace:*",
    "axios": "^1.1.3",
    "dayjs": "^1.11.6",
    "file-saver": "^2.0.5",
    "inputmask": "^5.0.7",
    "lodash": "^4.17.21",
    "sortablejs": "^1.15.0",
    "vuedraggable": "^4.0.3",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6",
    "element-plus": "2.2.21",
    "echarts": "^5.4.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "monaco-editor": "^0.34.1",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vitejs/plugin-vue-jsx": "^2.1.1",
    "@vue/compiler-sfc": "^3.2.45",
    "eslint": "^8.27.0",
    "eslint-plugin-vue": "^9.7.0",
    "sass": "^1.56.1",
    "typescript": "^4.8.4",
    "vite": "^3.2.3",
    "vite-svg-loader": "^3.6.0",
    "vue-global-api": "^0.4.1"
  },
  "files": [
    "dist/*"
  ],
  "module": "./src/index.ts",
  "style": "./dist/style.css",
  "types": "./dist/types/index.d.ts",
  "exports": {
    "./style.css": "./dist/style.css",
    ".": {
      "import": "./src/index.ts"
    }
  }
}